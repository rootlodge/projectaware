{
  "id": "research_workflow",
  "name": "Collaborative Research Project",
  "description": "Multi-agent workflow for comprehensive research tasks",
  "coordination": "parallel",
  "agents": ["researcher", "analyst", "fact_checker"],
  "steps": [
    {
      "name": "research_phase",
      "type": "agent_collaboration",
      "agents": ["researcher", "analyst"],
      "collaborationType": "discussion",
      "task": {
        "topic": "Research topic exploration",
        "maxRounds": 3
      }
    },
    {
      "name": "fact_verification",
      "type": "agent_task",
      "agentId": "fact_checker",
      "task": {
        "type": "fact_checking",
        "focus": "verification"
      },
      "input": {
        "source": "previous_step",
        "step": "research_phase"
      }
    },
    {
      "name": "final_synthesis",
      "type": "agent_collaboration",
      "agents": ["researcher", "analyst", "fact_checker"],
      "collaborationType": "consensus",
      "task": {
        "topic": "Research conclusions and recommendations",
        "maxRounds": 2
      }
    }
  ]
}
